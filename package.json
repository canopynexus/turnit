{
  "name": "@turnit/sources",
  "version": "0.0.0",
  "description": "TurnIt sources",
  "author": {
    "name": "Canopy Nexus Ltd",
    "url": "https://canopynexus.com"
  },
  "homepage": "https://github.com/canopynexus/turnit#readme",
  "bugs": {
    "url": "https://github.com/canopynexus/turnit/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/canopynexus/turnit.git"
  },
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=22"
  },
  "packageManager": "pnpm@9.15.4",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "test:coverage": "turbo run test:coverage",
    "test:watch": "turbo run test:watch",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "prettier": "3.5.3",
    "turbo": "2.4.4",
    "typescript": "5.7.3",
    "vitest": "3.0.7"
  }
}
